{
	"type": "light",
	"colors":
	{
		"editor.foreground": "#252525",
		"editor.background":"#DADADA",
		"sideBar.background": "#CECECE",
		"activityBar.background": "#DADADA",
		"activityBar.foreground": "#000000",
		"activityBar.border": "#00509F",
		"editor.lineHighlightBackground": "#C7C8C9",
		"editor.inactiveSelectionBackground": "#FFE1E1",
		"editor.selectionHighlightBackground": "#FFE1E1",
		"editorIndentGuide.background": "#646464"
	},
	"tokenColors":
	[
		{
			"scope" : "comment",
			"settings":
			{
				"foreground" : "#6666FF"
			}
		},
		{
			"scope" : "keyword",
			"settings":
			{
				"foreground" : "#00509F",
				"fontStyle" : "bold"
			}
		},
		{
			"scope" : "string",
			"settings":
			{
				"foreground" : "#99004C"
			}
		},
		{
			"name" : "numbers",
			"scope": 
			[
				"constant.numeric"
			],
			"settings": 
			{
				"foreground": "#3333FF"
			}
		},
		{
			"name" : "operators",
			"scope": 
			[
				"keyword.operator.assignment.cs",
				"keyword.operator.comparison.cs",
				"keyword.operator.arithmetic.cs",
				"keyword.operator.relational.cs",
				"keyword.operator.assignment.compound.bitwise.cs",
				"keyword.operator.assignment.compound.cs",
				"keyword.operator.null-coalescing.cs",
				"keyword.operator.logical.cs",
				"keyword.operator.conditional.question-mark.cs",
				"keyword.operator.bitwise.cs",
				"keyword.operator.decrement.cs",
				"keyword.operator.bitwise.shift.cs",
				"punctuation.separator.comma.cs"
			],
			"settings": 
			{
				"foreground": "#00509F"
			}
		}
	]
}